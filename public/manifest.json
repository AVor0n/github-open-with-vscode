{
    "manifest_version": 3,

    "name": "Github Open With",
    "description": "Allow open github repository in VSCode directly from browser",
    "version": "1.0",
    "action": {
        "default_icon": {
            "16": "icon.png"
        }
    },

    "content_scripts": [
        {
            "matches": ["https://github.com/"],
            "js": ["js/content_script.js"]
        }
    ]
}
